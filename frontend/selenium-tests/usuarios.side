{
  "id": "usuarios-tests",
  "version": "2.0",
  "name": "Usuarios Tests",
  "url": "http://localhost:3000",
  "tests": [
    {
      "id": "usuarios-list-load",
      "name": "Usuarios List Loads",
      "commands": [
        {
          "command": "open",
          "target": "/usuarios",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=.list-container",
          "value": "5000"
        },
        {
          "command": "assertElementPresent",
          "target": "css=.table-container",
          "value": ""
        }
      ]
    },
    {
      "id": "usuarios-search",
      "name": "Usuarios Search Functionality",
      "commands": [
        {
          "command": "open",
          "target": "/usuarios",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=input[placeholder*='Buscar']",
          "value": "5000"
        },
        {
          "command": "type",
          "target": "css=input[placeholder*='Buscar']",
          "value": "admin"
        },
        {
          "command": "waitForElementPresent",
          "target": "css=.table-container tbody tr",
          "value": "3000"
        }
      ]
    },
    {
      "id": "usuarios-add-button",
      "name": "Usuarios Add Button",
      "commands": [
        {
          "command": "open",
          "target": "/usuarios",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=button:has-text('Agregar')",
          "value": "5000"
        },
        {
          "command": "click",
          "target": "css=button:has-text('Agregar')",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=.edit-container",
          "value": "3000"
        }
      ]
    },
    {
      "id": "usuarios-edit",
      "name": "Usuarios Edit User",
      "commands": [
        {
          "command": "open",
          "target": "/usuarios",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=.table-container tbody tr:first-child",
          "value": "5000"
        },
        {
          "command": "click",
          "target": "css=.table-container tbody tr:first-child td:last-child button",
          "value": ""
        },
        {
          "command": "waitForElementPresent",
          "target": "css=.edit-container",
          "value": "3000"
        },
        {
          "command": "assertElementPresent",
          "target": "css=input[name='username']",
          "value": ""
        }
      ]
    }
  ],
  "suites": [
    {
      "id": "usuarios-suite",
      "name": "Usuarios Test Suite",
      "parallel": false,
      "persistSession": false,
      "tests": [
        "usuarios-list-load",
        "usuarios-search",
        "usuarios-add-button",
        "usuarios-edit"
      ]
    }
  ],
  "urls": [
    "http://localhost:3000/"
  ],
  "plugins": []
}
